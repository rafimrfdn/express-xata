  <h1>Delete User</h1>
  <% if (user) { %>
    <p>Are you sure you want to delete user "<%= user.nama %>" (<%= user.email %>) ?</p>
    <form hx-delete="/users/users/<%= user.id %>" hx-target="closest tr" method="DELETE">
      <button type="submit">Delete User</button>
    </form>
  <% } else { %>
    <p>User not found.</p>
  <% } %>
